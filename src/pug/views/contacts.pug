extends ../layout/_layout.pug

include /modules/ui/_breadcrumbs
include /modules/_hero-slider
include /modules/cards/_category-card
include /modules/_contact-form
include /modules/_partners
include /modules/_scroll-to-links

block variables
    - const title = 'Связаться с нами'

block content
    .container
        +breadcrumbs({
            items: [
                {
                    label: 'Главная',
                    attributes: {
                        href: '/',
                        itemprop: 'item',
                    }
                },
                {
                    label: 'Контакты',
                    attributes: {
                        href: '/contacts.html',
                        itemprop: 'item',
                    }
                },
            ]
        })
    +app-section({ title: 'Связаться с нами', arrows: true, pageTitle: true })(class="contacts")
        +app-grid(2)
            +app-grid
                .text-content
                    .contact-group._link
                        p.contact-group__label Телефон
                        a.contact-group__link(href="tel:+74993228940") +7(499) 322-89-40
                    .contact-group._link
                        p.contact-group__label Почта
                        a.contact-group__link(href="mailto:info@pikei.ru") info@pikei.ru
                    .contact-group
                        p.contact-group__label Отдел продаж
                        p.contact-group__item
                            span г.Москва, 22-й км Киевского шоссе, БП Румянцево, корпус В, подъезд 16, этаж 7, офис 730 В
                            span.faded= ` (режим работы пн. – пт.: 8.30 – 17.30)`
                    .contact-group
                        p.contact-group__label Склад
                        p.contact-group__item
                            span Московская область, г. Наро-Фоминск, ул. Чехова, д. 21 (режим работы пн. – пт.: 8.00 – 17.00)
                            span.faded= ` (режим работы пн. – пт.: 8.00 – 17.00)`
                +app-grid
                    p.text-lead Схема проезда на склад
                    +btn({
                        caption: 'Схема проезда',
                    })(data-fancybox, data-src="#directions")
            .contacts__map-container
                <iframe id="map_392191764" frameborder="0" width="100%" height="619px" sandbox="allow-modals allow-forms allow-scripts allow-same-origin allow-popups allow-top-navigation-by-user-activation"></iframe><script type="text/javascript">(function(e,t){var r=document.getElementById(e);r.contentWindow.document.open(),r.contentWindow.document.write(atob(t)),r.contentWindow.document.close()})("map_392191764", "")</script>



    +app-section({ title: 'Связаться с нами', variant: 'white', arrows: true, hasContainer: true, })(id="contact-us")
        +contact-form()

    .directions#directions(style='display:none;')
        .directions__image
            img(src='/i/c/contacts/directions.jpg', srcset="/i/c/contacts/directions.jpg 2x" alt="Схема проезда к складу")

        .directions__actions
            +btn({
                caption: 'Распечатать',
            })(id="print", onclick="printJS('/i/c/contacts/directions.jpg', 'image')")
            +btn({
                href: '/i/c/contacts/directions.jpg',
                caption: 'Поделиться',
            })(data-text="%сюда вставить адрес склада%", data-title="Схема проезда к складу", class="js-share")


